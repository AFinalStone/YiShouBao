<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:paddingTop="18dp"
    android:paddingBottom="18dp"
    android:background="@color/colorWhite_FFFFFFFF"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/iv_model"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@mipmap/icon_empty"/>

    <TextView
        android:id="@+id/tv_modelName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="安全防护"
        android:layout_marginLeft="10dp"
        android:textColor="@color/colorBlack_FF333333"
        android:textSize="@dimen/textFontSize_14" />
</LinearLayout>